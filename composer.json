{
  "name": "neoflow/data",
  "description": "Data handler for arrays.",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Jonathan Nessier",
      "homepage": "https://www.neoflow.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Neoflow\\Data\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neoflow\\Data\\Test\\": "tests/"
    }
  },
  "scripts": {
    "run php-cs-fixer": "php-cs-fixer fix",
    "run php-cs-fixer dry": "php-cs-fixer fix --dry-run",
    "run phpunit": "phpunit --configuration tests/phpunit.xml --bootstrap vendor/autoload.php"
  },
  "require": {
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "php-coveralls/php-coveralls": "^2.2",
    "pds/skeleton": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.13"
  }
}
